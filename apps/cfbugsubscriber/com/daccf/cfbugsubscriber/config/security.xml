<?xml version="1.0" encoding="ISO-8859-1"?>
<rules>
	<rule>
		<match>event</match>
		<whitelist>user\..*,account\..*,main\.message</whitelist>
		<securelist>.*\..*</securelist>
		<roles>user</roles>
		<redirect>user.login</redirect>
	</rule>
	<rule>
		<match>event</match>
		<whitelist></whitelist>
		<securelist>account\.update</securelist>
		<roles>user</roles>
		<redirect>user.login</redirect>
	</rule>
</rules>