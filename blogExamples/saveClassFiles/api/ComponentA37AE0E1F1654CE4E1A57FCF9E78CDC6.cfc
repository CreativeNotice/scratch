	<cffunction name="init" returntype="ComponentA37AE0E1F1654CE4E1A57FCF9E78CDC6" access="public" output="false" hint="Initialises and returns the object">
		<cfargument name="configFile" type="string" required="true" hint="Config file name">

		<cfset var jsonConfig = "">

