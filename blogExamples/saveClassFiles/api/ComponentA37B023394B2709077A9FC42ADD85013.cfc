	<cffunction name="init" returntype="ComponentA37B023394B2709077A9FC42ADD85013" access="public" output="false" hint="Initialises and returns the object">
		<cfargument name="configFile" type="string" required="true" hint="Config file name">

		<cfset var jsonConfig = "">

