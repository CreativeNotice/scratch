	<cffunction name="init" returntype="ComponentA37B38E402F56B4A922D32BA5F7D2637" access="public" output="false" hint="Initialises and returns the object">
		<cfargument name="configFile" type="string" required="true" hint="Config file name">

		<cfset var jsonConfig = "">

