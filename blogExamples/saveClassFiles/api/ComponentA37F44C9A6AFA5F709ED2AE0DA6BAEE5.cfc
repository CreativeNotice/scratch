	<cffunction name="init" returntype="ComponentA37F44C9A6AFA5F709ED2AE0DA6BAEE5" access="public" output="false" hint="Initialises and returns the object">
		<cfargument name="configFile" type="string" required="true" hint="Config file name">

		<cfset var jsonConfig = "">

